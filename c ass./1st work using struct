#include <stdio.h>
#include <string.h>

struct Course{
    float mid;
    float final;
    float perf;
};

struct Student{
    char name[100];
    short int roll;
   struct Course course1;
   struct Course course2;
    struct Course course3;
}std[20];

void struct_student();

int main(){ 
    int cgpa;
struct_student_func();

for (int i=0; i<20; i++){

printf("Name: %s\nRoll: %hd\nCGPA: ", std[i].name, std[i].roll);

  cgpa =((((std[i].course1.mid+std[i].course2.mid+std[i].course3.mid)*30/72)+((std[i].course1.final+std[i].course2.final+std[i].course3.final)*50/120)+((std[i].course1.perf+std[i].course2.perf+std[i].course3.perf)*20/100)))/3;
  
  if(cgpa>=80 && cgpa<=100) printf("A+");
  else if(cgpa>=75 && cgpa<80) printf("A");
  else if(cgpa>=70 && cgpa<74) printf("A-");
  else if(cgpa>=55 && cgpa<69) printf("B");
  else if(cgpa>=45 && cgpa<54) printf("C");
  else if(cgpa>=40 && cgpa<45) printf("D");
  else  printf("F");
  
printf("\n\n");
}
return 0;
}

